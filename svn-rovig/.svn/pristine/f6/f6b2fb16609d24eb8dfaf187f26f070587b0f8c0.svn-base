\begin{frame}
\frametitle{Dual stress-based formulation of Contact with Friction}
Minimize the energy functional:
 \begin{align*}\colo
 \mathcal{J}(\bsigma) :=  \frac{1}{2} \int_\Omega \left( \mathcal{A} \bsigma \right) : \bsigma \: dx  \colo 
 - \int_{\Gamma_D} \bu_D \cdot (\bsigma \cdot \bn) \: ds - \int_{\Gamma_C} g \: (\bn \cdot (\bsigma \cdot \bn)) \: ds
\end{align*}
 %\mbox{ \colk subject to: } \colo \div \; \bsigma + \bff & \colo = \bzero \mbox{ \colk in } \Omega\\[-0.5em] 
    %                            \colo \as \; \bsigma & \colo = \bzero \mbox{ \colk in } \Omega \\
    %						   \colo \bsigma \cdot \bn  & \colo =  \bzero \mbox{ \colk on } \Gamma_N
among all $\colo \bsigma \in \bSigma({\colb \bsigma}) := \{ \btau \in \bSigma: \: \div \; \btau + \bff = \bzero, \:  \as \; \btau = \bzero, $ \\[0.5em] 
\hspace{2cm} $\colo \bn \cdot (\btau \cdot \bn) \leq 0,  \: | \bn \times (\btau \cdot \bn) | \leq \mu_F \colb | \bn \cdot (\bsigma \cdot \bn) | \: \colo \text{on } \Gamma_C\}$ \\[1em] 
\begin{center} $\boldsymbol{\Longleftrightarrow}$ \end{center} 
Determine 
$\colo \bsigma \in \bSigma({\colb \bsigma})$ such that:
\begin{align*}
 \colo \left( \mathcal{A} \bsigma  , \btau-\bsigma \right)_\Omega
 \geq \left\langle \bu_D, (\btau-\bsigma) \cdot \bn \right\rangle_{\Gamma_D} 
 +     \left\langle g , \bn \cdot ((\btau-\bsigma) \cdot \bn)  \right\rangle_{\Gamma_C}
\end{align*}
holds for all 
$\colo \btau \in \bSigma({\colb \bsigma})$.
\end{frame}


\begin{frame}
\frametitle{Coulomb Friction as limit of Tresca Iteration}
\framesubtitle{for small $\mu_F$}
Minimize the energy functional:
 \begin{align*}\colo
 \mathcal{J}(\bsigma) :=  \frac{1}{2} \int_\Omega \left( \mathcal{A} \bsigma \right) : \bsigma \: dx  \colo 
 - \int_{\Gamma_D} \bu_D \cdot (\bsigma \cdot \bn) \: ds - \int_{\Gamma_C} g \: (\bn \cdot (\bsigma \cdot \bn)) \: ds
\end{align*}
among all $\colb \bsigma^n \colo \in \bSigma({\colb \bsigma^{n-1}}) := \{ \btau \in \bSigma: \: \div \; \btau + \bff = \bzero, \:  \as \; \btau = \bzero,$ \\[0.5em] 
\hspace{1.5cm} $\colo \bn \cdot (\btau \cdot \bn) \leq 0,  \: | \bn \times (\btau \cdot \bn) | \leq \mu_F \colb | \bn \cdot (\bsigma^{n-1} \cdot \bn) | \: \colo \colo \text{on } \Gamma_C\}$ \\[1em] 
\begin{center} $\boldsymbol{\Longleftrightarrow}$ \end{center} 
Determine 
$\colb \bsigma^n \colo \in \bSigma({\colb \bsigma^{n-1}})$ such that:
\begin{align*}
 \colo \left( \mathcal{A} {\colb \bsigma^n}  , \btau-{\colb \bsigma^n} \right)_\Omega
 \geq \left\langle \bu_D, (\btau-{\colb \bsigma^n}) \cdot \bn \right\rangle_{\Gamma_D} 
 +     \left\langle g , \bn \cdot ((\btau-{\colb \bsigma^n}) \cdot \bn)  \right\rangle_{\Gamma_C}
\end{align*}
holds for all 
$\colo \btau \in \bSigma({\colb \bsigma^{n-1}})$.
\end{frame}

\begin{frame}
  \frametitle{Displacement Reconstruction in $H^1$}
  %\framesubtitle{}
  
  \vspace*{-0.5cm}
  
  \[\colo
    - ( \bu_h , \div \, \btau_h )  = ( \cA \bsigma_h , \btau_h ) + ( \btheta_h , \as \, \btau_h )
    = ( \cA \bsigma_h + \btheta_h , \btau_h ) =: ( \bz_h , \btau_h )
  \]
  $\colo \bz_h$:  discontinuous piecewise polynomial approximation to $\colo \bnabla \bu_h$
  
  \vspace*{0.25cm}
%\pause
  Gradient Reconstruction Algorithm: \\
  {\em Step 1.} For each $\colo T \in \cT_h$, determine $\colo \left. \bu_h^\circ \right|_T \in \cP_k (T)^d$
  \begin{align*} \colo
    ( \bnabla \bu_h^\circ , \bnabla \bv_h )_{L^2 (T)} & \colo = ( \bz_h , \bnabla \bv_h )_{L^2 (T)} \mbox{ for all } \bv_h \in \cP_k (T)^d \\ \colo
    ( \bu_h^\circ , \be_i )_{L^2 (T)} & \colo = ( \bu_h , \be_i )_{L^2 (T)} \mbox{ for } 1 \leq i \leq d
  \end{align*} %\pause
  {\em Step 2.} Construct $\colo \bu_h^R \in \cP_k (\cT_h)^d \subset H^1 (\Omega)^d$ by averaging:
  \[ \colo
    \bu_h^R (\bx) = \frac{1}{\# \{ T : \bx \in T \}} \left( \sum_{T : \bx \in T} \left. \bu_h^\circ \right|_T (\bx) \right)
  \]
  {\scriptsize \colg Kim, Appl. Math. Comput. (2012)}\\
  {\scriptsize \colg Vohral\:\!\!{\'{}}\!\!ik, Math. Comp. (2010)}\\
  {\scriptsize \colg Stenberg, RAIRO Math. Model. Numer. Anal. (1991)}
  
\end{frame}

\begin{frame}
  \frametitle{Reconstruction-Based A Posteriori Error Estimator}
  \framesubtitle{}
 
  
  \vspace*{-0.5cm}
  
  \[ \colo
    \eta (\bsigma_h,\bu_h^R) = \| \cA \bsigma_h - \bepsilon (\bu_h^R) \|_{L^2 (\Omega)}
    \begin{array}{l}
      \gtrsim \| \bsigma - \bsigma_h \|_{H (\div,\Omega)} \mbox{ (reliability)} \\
      \lesssim \| \bsigma - \bsigma_h \|_{H (\div,\Omega)} \mbox{ (efficiency)}
    \end{array}
  \]
  with constants which are independent of $ \colo \lambda \rightarrow \infty$
  \vspace*{0.25cm}
  %\pause
  %Signorini contact conditions:
  %\begin{align*}
   % & \colo \left\{
   % \begin{array}{l l}
      %\bn \cdot (\bsigma \cdot \bn) & \colo \leq 0 \\ \colo
      %\bn \cdot \bu - g &  \colo \leq 0
    %\end{array}
  %\right\} \mbox{ on } \Gamma_C
  %\end{align*}

  Add complementarity terms to the error estimator:
  \begin{align*}   \colo
    \eta^{CF} (\bsigma_h)^2
    & \colo = \| \cA \bsigma_h - \bepsilon(\bu_h^R) \|_{L^{2}(\Omega)}^{2}
    + \langle \bn \cdot \bu_h^R - g , \bn \cdot (\bsigma_h \cdot \bn) \rangle_{\Gamma_{C}}\\
    & \colo \hspace*{0.4 cm} + \langle \bn \times \bu_h^R , \bn \times (\bsigma_h \cdot \bn) \rangle_{\Gamma_{C}}
    + \langle | \bn \times \bu_h^R | , \mu_F | \bn \cdot (\bsigma_h \cdot \bn) | \rangle_{\Gamma_{C}}
  \end{align*}  
  
  %\vspace{-0.5cm} %\pause


  Reliability (for sufficiently small friction coefficient $\mu_F$):
  \begin{align*}
    & \colo \hspace{2cm} \quad \eta^{CF} (\bsigma_h) \gtrsim \| \bsigma^* - \bsigma_h \|_{H (\div,\Omega)} \quad
    \mbox{ \colk on the constraint set}\\
    & \hspace{-0.5cm}\colo \left\{ \bn \cdot \bu_h^R - g \leq 0, \: \bn \cdot (\bsigma_h \cdot \bn) \leq 0,\:
    |\bn \times (\bsigma_h \cdot \bn)| \leq \mu_F |\bn\cdot(\bsigma_h\cdot\bn)|\right\} \text{ on } \Gamma_{C}
  \end{align*}
  The use of such a functional for error estimation goes back to \\ (at least) {\colg \scriptsize I. Hlav\'a\v{c}ek, J. Haslinger, J. Ne\v{c}as, J. Lov\'{i}\v{s}ek: Springer, 1988}
\end{frame}


